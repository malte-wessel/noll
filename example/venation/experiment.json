{
    "title": "Venation",
    "width": 800,
    "height": 600,
    "update": "./update.js",
    "initialize": "./initialize.js",
    "controls": [{
        "key": "palette",
        "label": "Palette",
        "type": "select",
        "value": "light",
        "options": [
            { "value": "light", "label": "Light" },
            { "value": "dark", "label": "Dark" }
        ]
    }, {
        "key": "showSeeds",
        "label": "Show seeds",
        "type": "boolean",
        "value": true
    }, {
        "key": "showRadiusOfInfluence",
        "label": "Show radius of influence",
        "type": "boolean",
        "value": false
    }, {
        "key": "rootsCount",
        "label": "Number of roots",
        "type": "number",
        "value": 1,
        "min": 1,
        "max": 100,
        "step": 1
    }, {
        "key": "seedsCount",
        "label": "Number of seeds",
        "type": "number",
        "value": 15000,
        "min": 100,
        "max": 50000,
        "step": 100
    }, {
        "key": "radiusOfInfluence",
        "label": "Radius of influence",
        "type": "number",
        "value": 10,
        "min": 1,
        "max": 150,
        "step": 1
    }, {
        "key": "nodeLength",
        "label": "Node length",
        "type": "number",
        "value": 5,
        "min": 1,
        "max": 20,
        "step": 1
    }, {
        "key": "maxDepth",
        "label": "Max node depth",
        "type": "number",
        "value": 1000,
        "min": 1,
        "max": 2000,
        "step": 10
    }, {
        "key": "maxChilds",
        "label": "Max child nodes",
        "type": "number",
        "value": 3,
        "min": 1,
        "max": 10,
        "step": 1
    }, {
        "key": "minSiblingDistance",
        "label": "Min sibling distance",
        "type": "number",
        "value": 5,
        "min": 0,
        "max": 10,
        "step": 1
    }, {
        "key": "killDistance",
        "label": "Kill distance",
        "type": "number",
        "value": 1.0,
        "min": 0.5,
        "max": 1.5,
        "step": 0.1
    }, {
        "key": "iterationsPerFrame",
        "label": "Iterations per frame",
        "type": "number",
        "value": 75,
        "min": 1,
        "max": 200,
        "step": 1
    }]
}
